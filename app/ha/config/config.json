            "default": 3,
            "min":0,
            "max":30,
            "changeable": true,
            "required": "no"
          },
          {
            "key": "docker_prune_days",
            "label": "Docker prune days",
            "description": "Keep docker for x days, 0 indicates that the log is not cleared",
            "type": "integer",
            "default": 3,
            "min":0,
            "max":30,
            "changeable": true,
            "required": "no"
          },
          {
            "key": "kube_log_level",
