apiVersion: storage.k8s.io/v1
kind: StorageClass
metadata:
  name: csi-qingcloud-capacity
  annotations:
    storageclass.kubesphere.io/supported_access_modes: '["ReadWriteOnce"]'
provisioner: csi-qingcloud
parameters:
  type: "2"
  maxSize: "5000"
  minSize: "100"
  stepSize: "50"
  fsType: "ext4"
reclaimPolicy: Delete